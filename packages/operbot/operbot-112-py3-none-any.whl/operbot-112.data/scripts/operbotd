"write your own commands"


import inspect


from opr import Command, Wd, scan, wait
from operbot import cmd, irc, rss


Wd.workdir = "/var/lib/operbot/"


def main():
    scan(cmd)
    scan(irc)
    scan(rss)
    irc.init()
    rss.init()
    wait()


main()
